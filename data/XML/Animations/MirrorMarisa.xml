<animation_set>
    <id_string>MirrorMarisa</id_string>
    <animation_filename>enemy-mirror-marisa.png</animation_filename>
    
    <!--Standing Frame-->
    <animation_sequence type="idle" repeat="True">
        <frame>
	    <frame_coord>(0,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="physical" repeat="False">
        <frame>
	    <frame_coord>(0,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
        <frame>
	    <frame_coord>(1,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
        <frame>
	    <frame_coord>(2,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
        <frame>
	    <frame_coord>(3,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
        <frame>
	    <frame_coord>(4,0)</frame_coord>
            <coords>(25,0)</coords>
        </frame>

    </animation_sequence>
    
    <animation_sequence type="magical" repeat="False">
        <frame>
	    <frame_coord>(0,1)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
        <frame>
	    <frame_coord>(1,1)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
        <frame>
	    <frame_coord>(2,1)</frame_coord>
            <coords>(-5,0)</coords>
        </frame>
        <frame>
	    <frame_coord>(3,1)</frame_coord>
            <coords>(-10,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="cast_physical" repeat="False">
        <frame>
	    <frame_coord>(4,0)</frame_coord>
            <coords>(25,0)</coords>
        </frame>
    </animation_sequence>
    <animation_sequence type="cast_magical" repeat="False">
        <frame>
	    <frame_coord>(3,1)</frame_coord>
            <coords>(-10,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="dodge" repeat="False">
    </animation_sequence>
    
    <animation_sequence type="hit" repeat="False">
    </animation_sequence>
    
</animation_set>
