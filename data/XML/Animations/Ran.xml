	    <frame>
    		<frame_coord>(0,0)</frame_coord>
	    	<coords>(0,0)</coords>
	    </frame>
    </animation_sequence>
    
    <animation_sequence type="physical" repeat="False">
	<frame>
  	   <frame_coord>(0,0)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(1,0)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	
	<frame>
  	   <frame_coord>(2,0)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(3,0)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="magical" repeat="False">
        <frame>
  	   <frame_coord>(0,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(1,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	
	<frame>
  	   <frame_coord>(2,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(3,1)</frame_coord>
