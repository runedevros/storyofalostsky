        </frame>
	<frame>
  	   <frame_coord>(4,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(5,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="cast_physical" repeat="False">
	<frame>
  	   <frame_coord>(3,0)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
    </animation_sequence>


    <animation_sequence type="cast_magical" repeat="True">
	<frame>
  	   <frame_coord>(2,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(3,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(4,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
	<frame>
  	   <frame_coord>(5,1)</frame_coord>
       	   <coords>(0,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="dodge" repeat="False">
    </animation_sequence>
    
    <animation_sequence type="hit" repeat="True">
    </animation_sequence>
    
</animation_set>
