<animation_set>
    <id_string>WalkingTree</id_string>
    <animation_filename>enemy-tree.png</animation_filename>
    
    <!--Standing Frame-->
    <animation_sequence type="idle" repeat="False">
        <frame>
            <frame_coord>(0,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
    </animation_sequence>
    
    
    <animation_sequence type="physical" repeat="False">
        <frame>
            <frame_coord>(0,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="magical" repeat="False">
        <frame>
            <frame_coord>(0,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
    </animation_sequence>

    <animation_sequence type="cast_physical" repeat="True">
        <frame>
            <frame_coord>(0,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
    </animation_sequence>

    <animation_sequence type="cast_magical" repeat="True">
        <frame>
            <frame_coord>(0,0)</frame_coord>
            <coords>(0,0)</coords>
        </frame>
    </animation_sequence>
    
    <animation_sequence type="dodge" repeat="False">
    </animation_sequence>
    
    <animation_sequence type="hit" repeat="False">
    </animation_sequence>
    
</animation_set>
